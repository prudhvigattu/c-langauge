#include <stdio.h>
int main()
{
	int num1,num2;
	char op;
	printf("Enter num1:");
	scanf("%d",&num1);
	printf("\nEnter num2:");
	scanf("%d",&num2);
	printf("\nEnter operator:");
	scanf(" %c",&op);
	l1:switch(op)
	{
		case '+':printf("%d",(num1+num2));
		break;
		case '-':printf("%d",(num1-num2));
		break;
		case '*':printf("%d",(num1*num2));
		break;
		case '/':printf("%d",(num1/num2));
		break;
		default:
			printf("invalid operator");
		
	}
}
